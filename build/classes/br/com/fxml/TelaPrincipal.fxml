<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.fxml.controllers.TelaPrincipalController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="900.0" nodeOrientation="INHERIT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@imagens/fundo.jpg" />
         </image>
      </ImageView>
      <MenuBar focusTraversable="false" prefWidth="900.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="Contábil">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#paneImportacoes" text="Importações" />
               </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Fiscal">
              <items>
                <MenuItem mnemonicParsing="false" text="DePara" />
              </items>
            </Menu>
        </menus>
         <effect>
            <InnerShadow />
         </effect>
      </MenuBar>
      <Pane fx:id="telaPrincipal" layoutX="50.0" layoutY="50.0" prefHeight="500.0" prefWidth="800.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0" />
   </children>
</AnchorPane>
